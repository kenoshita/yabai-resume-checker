{
  "headers": [
    {
      "source": "**/*.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "source": "**/*.(css|js)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "status": 200
    }
  ]
}